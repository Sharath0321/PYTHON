import json

with open('weather_data.json') as f:
    data=json.load(f)
    
    curr_temp=data['main']['temp']
    humidity =data['main']['humidity']
    weather_desc = data['weather'][0]['description']
    
    print(f"Current temperature :{curr_temp} c")
    print(f"Humidity : {humidity}%")
